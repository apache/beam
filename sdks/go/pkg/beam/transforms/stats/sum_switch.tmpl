    "fmt"
    "reflect"
)

func findSumFn(t reflect.Type) interface{} {
    switch t.String() {
{{- range .X}}
    case "{{.Type}}":
		return sum{{.Name}}Fn
{{- end}}
	default:
		panic(fmt.Sprintf("Unexpected number type: %v", t))
	}
}
{{range .X}}
func sum{{.Name}}Fn(x, y {{.Type}}) {{.Type}} {
	return x + y
}
{{end}}
