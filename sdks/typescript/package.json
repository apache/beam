{
  "name": "apache-beam",
  "version": "2.50.0-SNAPSHOT",
  "devDependencies": {
    "@google-cloud/bigquery": "^5.12.0",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
