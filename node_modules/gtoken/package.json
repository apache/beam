{
  "name": "gtoken",
  "version": "6.1.1",
  "description": "Node.js Google Authentication Service Account Tokens",
  "main": "./build/src/index.js",
  "types": "./build/src/index.d.ts",
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": "google/node-gtoken",
  "scripts": {
