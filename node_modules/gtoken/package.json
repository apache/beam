    "jws": "^4.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.7",
    "@types/jws": "^3.1.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.0.0",
    "c8": "^7.0.0",
    "gts": "^3.0.0",
    "linkinator": "^4.0.0",
    "mocha": "^9.2.2",
    "nock": "^13.0.0",
