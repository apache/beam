{
  "name": "google-auth-library",
  "version": "8.3.0",
  "author": "Google Inc.",
  "description": "Google APIs Authentication Client Library for Node.js",
  "engines": {
    "node": ">=12"
  },
  "main": "./build/src/index.js",
  "types": "./build/src/index.d.ts",
  "repository": "googleapis/google-auth-library-nodejs.git",
  "keywords": [
